{
  "expo": {
    "name": "Oh My Poop",
    "slug": "oh-my-poop",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "com.chad.ohmypoop",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3D4127"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chad.ohmypoop",
      "buildNumber": "8",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3D4127"
      },
      "package": "com.chad.ohmypoop"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-font", "expo-apple-authentication"],
    "fonts": [
      {
        "asset": "./assets/fonts/Pattaya-Regular.ttf",
        "family": "Pattaya"
      }
    ],
    "extra": {
      "eas": {
        "projectId": "afb84fb1-58e5-4293-a136-8c1d25f12332"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/afb84fb1-58e5-4293-a136-8c1d25f12332"
    }
  }
}
