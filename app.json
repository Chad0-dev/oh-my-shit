{
  "expo": {
    "name": "Oh My Poop",
    "slug": "oh-my-shit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3D4127"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ohmyshit.dev",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3D4127"
      },
      "package": "com.ohmyshit.dev"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-font"],
    "fonts": [
      {
        "asset": "./assets/fonts/Pattaya-Regular.ttf",
        "family": "Pattaya"
      }
    ],
    "extra": {
      "eas": {
        "projectId": "a885c728-5d25-4658-aa00-3ae1bfced4cf"
      }
    }
  }
}
